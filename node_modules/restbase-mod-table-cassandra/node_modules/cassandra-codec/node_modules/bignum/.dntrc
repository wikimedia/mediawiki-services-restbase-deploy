## DNT config file
## see https://github.com/rvagg/dnt

NODE_VERSIONS="\
  v0.10.35
"
IOJS_VERSIONS="\
  v1.0.1-release \
"
OUTPUT_PREFIX="bignum-"
TEST_CMD="\
  cd /dnt/ &&                                                    \
  npm install node-gyp &&                                        \
  npm install &&                                                 \
  node_modules/.bin/node-gyp --nodedir /usr/src/node/ rebuild && \
  npm test;                          \
"
LOG_OK_CMD="sed 's/^   ..32m100...0m .. tests$/ok/' | tail -2 | head -1"
